{
  "list": [
    {
      "name" : "Task ftp.parttrade.ru",
      "type" : "ftp",
      "host" : "ftp.parttrade.ru",
      "user" : "auto.ru",
      "pass" : "auto.ru",
      "path" : ".",
      "user_id" : 1575,
      "file_extension_to_download" : [],
      "file_list" : [
        "autogiper_all.zip"
      ],
      "file_mask" : null,
      "file_extension_to_processed" : ["csv"],
      "csv_opts" : {
        "headers"   : false,
        "delimiter" : ",",
        "quote "    : "\"",
        "escape"    : "\\",
        "objectMode": true
      },
      "transform_opts" : {
        "manufacturer" : {
          "column" : 0
        },
        "code" : {
          "column" : 1,
          "type" : "number_latter"
        },
        "name" : {
          "column" : 2
        },
        "count" : {
          "column" : 3,
          "type" : "to_int",
          "cancel": "NaN"
        },
        "price" : {
          "column" : 4,
          "type" : "to_int"
        },
        "delivere" : {
          "column" : 5,
          "type" : "regexp",
          "value": "[^0-9\\-]",
          "options": "gi"
        }
      }
    }
  ],
  "check_interval": 864e5,
  "max_file_transmission" : 10,
  "db_connection_string" : "postgres://autogiper:autogiper@localhost/autogiper",
  "temp_folder" : "./temp"
}